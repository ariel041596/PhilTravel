<?xml version='1.0' encoding='utf-8'?>
<widget id="org.cordova.quasar.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PhilTravel</name>
    <description>A guide for travelling in Philippines</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
            <icon density="ldpi" src="app-icon.png" />
            <icon density="mdpi" src="app-icon.png" />
            <icon density="xxxhdpi" src="app-icon.png" />
            <splash density="land-ldpi" src="app-icon.png" />
            <splash density="port-ldpi" src="app-icon.png" />
            <splash density="land-mdpi" src="app-icon.png" />
            <icon density="hdpi" src="app-icon.png" />
            <icon density="xxhdpi" src="app-icon.png" />
            <splash density="port-mdpi" src="app-icon.png" />
            <splash density="land-hdpi" src="app-icon.png" />
            <splash density="land-xxhdpi" src="app-icon.png" />
            <splash density="port-xxhdpi" src="app-icon.png" />
            <splash density="land-xxxhdpi" src="app-icon.png" />
            <splash density="port-xxxhdpi" src="app-icon.png" />
            <icon density="xhdpi" src="app-icon.png" />
            <splash density="port-hdpi" src="app-icon.png" />
            <splash density="land-xhdpi" src="app-icon.png" />
            <splash density="port-xhdpi" src="app-icon.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <allow-navigation href="about:*" />
</widget>
